{
  "name": "wingmercs",
  "version": "1.0.0",
  "description": "wing mercs native builder",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . wingmercs --platform=darwin --arch=x64 --overwrite",
    "package-mac-arm": "electron-packager . wingmercs --platform=darwin --arch=arm64 --overwrite",
    "package-win": "electron-packager . wingmercs --platform=win32 --arch=x64 --overwrite",
    "copy-assets": "cp -R ../wingmerc/dist dist"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "electron-packager": "^17.1.2",
    "electron": "^29.1.0"
  }
}
